<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.btn{
			width: 150px;
		}
	</ui:style>
	<g:HTMLPanel>
	<style>
	.gwt-SuggestBoxPopup{
			z-index: 10000;
		}
	</style>
	<m:MaterialPanel>
		<m:MaterialModalContent >
			<m:MaterialAutoComplete ui:field="motsCles" placeholder="Mots Cles"  />
		</m:MaterialModalContent>
		<m:MaterialModalFooter align="RIGHT">
			<m:MaterialButton ui:field="btnAgree" waves="GREEN" type="FLAT" text="Valider" addStyleNames="{style.btn}" />
			<m:MaterialButton ui:field="btnDisagree" waves="RED" type="FLAT" text="Annuler" addStyleNames="{style.btn}" />
		</m:MaterialModalFooter>
	</m:MaterialPanel>
	</g:HTMLPanel>
</ui:UiBinder> 